{
  "name": "music-export",
  "version": "1.0.0",
  "author": "Andrey Borha",
  "license": "MIT",
  "scripts": {
    "start:client": "webpack-dev-server --config ./src/weback/webpack.client.config.js",
    "start:server": "nest start",
    "start:server:dev": "nest start --watch",
    "start:server:debug": "nest start --debug --watch",
    "build:client": "rimraf dist/client && webpack --config ./src/weback/webpack.client.config.js",
    "build:server": "rimraf dist/server && nest build",
    "docker:client:build-image": "docker build -t music-export-client -f ./docker/client/Dockerfile .",
    "docker:server:build-image": "docker build -t music-export-server -f ./docker/server/Dockerfile .",
    "docker:client:run:": "docker run -d --rm -p 80:5000 --name=music-export-client music-export-client",
    "docker:server:run:": "docker run -d --rm -p 3000:3000 --name=music-export-server music-export-server",
    "docker:compose": "docker-compose up"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/platform-express": "^6.10.14",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "serve": "^11.3.0",
    "vue": "^2.6.11",
    "vue-material": "^1.0.0-beta-14"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@nestjs/cli": "^6.14.0",
    "@nestjs/schematics": "^6.8.2",
    "@types/express": "^4.17.2",
    "@webpack-cli/init": "^0.2.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "css-loader": "^3.4.1",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "terser-webpack-plugin": "^2.3.1",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "description": "Music export app"
}
