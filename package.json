{
  "name": "music-export",
  "version": "1.0.0",
  "author": "Andrey Borha",
  "license": "MIT",
  "scripts": {
    "start:client": "webpack-dev-server --config src/webpack/webpack.client.config.js",
    "start:server": "nest start",
    "start:server:dev": "nest start --watch",
    "start:server:debug": "nest start --debug --watch",
    "build:client": "rimraf dist/client && webpack --config src/webpack/webpack.client.config.js",
    "build:server": "rimraf dist/server && nest build",
    "docker:client:build-image": "docker build -t music-export-client -f ./docker/client/Dockerfile .",
    "docker:server:build-image": "docker build -t music-export-server -f ./docker/server/Dockerfile .",
    "docker:client:run:": "docker run -d --rm -p 80:5000 --name=music-export-client music-export-client",
    "docker:server:run:": "docker run -d --rm -p 3000:3000 --name=music-export-server music-export-server",
    "docker:compose": "docker-compose up"
  },
  "dependencies": {
    "@nestjs/common": "^7.2.0",
    "@nestjs/core": "^7.2.0",
    "@nestjs/platform-express": "^7.2.0",
    "mobx": "^5.15.4",
    "mobx-vue": "^2.0.10",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "serve": "^11.3.0",
    "typescript": "^3.9.5",
    "vue": "^2.6.11",
    "vue-material": "^1.0.0-beta-14"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@nestjs/cli": "^7.4.0",
    "@nestjs/schematics": "^7.0.1",
    "@types/express": "^4.17.2",
    "@webpack-cli/init": "^0.3.0",
    "babel-loader": "^8.1.0",
    "core-js": "3.6.5",
    "css-loader": "^3.4.1",
    "eslint": "^7.2.0",
    "eslint-plugin-vue": "^6.1.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "terser-webpack-plugin": "^3.0.6",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "description": "Music export app"
}
